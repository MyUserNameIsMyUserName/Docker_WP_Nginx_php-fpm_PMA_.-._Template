    "_create_folders": " bash -c \" mkdir -p addon_files/certs/ addon_files/certs-data/ addon_files/logs/nginx/ mysql/ PUBLIC/ \" ",
    "_build_containers": "docker-compose up -d",
    "_stop_and_clean": " bash ./addon_files/stop_and_clean.sh ",
